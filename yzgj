            //成品检验，隐蔽检验
            checkDetail: function (checkItems, checkResult, jlcheckResult, domId) {
                var self =this;
                var keyArr = [];
                var checkList = [];
                if(checkItems && Object.values(checkItems).length>0) {
                    for (var key in checkItems) {
                        var list = [];
                        for (var key1 in checkItems[key]) {
                            list.push({
                                text: checkItems[key][key1],
                                indent: false,
                                value: key1
                            })
                        }
                        checkList.push({
                            label: key,
                            list: list,
                            type: false,
                            type1: false,
                        })
                        keyArr.push(key);
                    }
                }
                if(checkResult && Object.values(checkResult).length>0 &&checkList)  {
                    for (var item in checkResult) {
                        for (var item1 = 0; item1 < checkList.length; item1++) {
                            for (var item2 = 0; item2 < checkList[item1].list.length; item2++) {
                                if (checkList[item1].list[item2].value == item) {
                                    checkList[item1].list[item2].indent = true
                                    checkList[item1].type = true
                                }
                            }
                        }
                    }
                }
                // 监理检验项判断是否合格
                if(jlcheckResult && Object.values(jlcheckResult).length>0){
                    for (var item in jlcheckResult) {
                        for (var item1 = 0; item1 < checkList.length; item1++) {
                            for (var item2 = 0; item2 < checkList[item1].list.length; item2++) {
                                if (checkList[item1].list[item2].value == item) {
                                    checkList[item1].list[item2].indent = true
                                    checkList[item1].type1 = true
                                }
                            }
                        }
                    }
                    if (jlcheckResult.PICTURE) {
                        let imageUrl = [];
                        jlcheckResult.PICTURE.split(',').forEach(function (item) {
                            imageUrl.push(self.filterFile(item).url)
                        })
                        checkResult.PICTURE1 = imageUrl
                    }
                }
                if (checkResult.PICTURE) {
                    let imageUrl = [];
                    checkResult.PICTURE.split(',').forEach(function (item) {
                        imageUrl.push(self.filterFile(item).url)
                    })
                    checkResult.PICTURE = imageUrl
                }
                if (domId == 'hiddenCheck_detail') {
                    this.hiddenCheck_detail = [{
                        checkResult,
                        checkList
                    }]
                } else if (domId == 'product_detail') {
                    this.product_detail = [{
                        checkResult,
                        checkList
                    }]
                } else {
                    this.sendCheckReasonMap = [{
                        checkResult,
                        checkList
                    }]
                }
            },
            // 跳转组模详情
            goToModuleDetails: function (type, options) {
                var rawMaterialsMap =this.rawMaterialsMap
                var title = '隐检详情'
                if (type == 1) {
                    title = options
                }
                if (type == 1 && rawMaterialsMap[options] && !rawMaterialsMap[options].length) return
                if (type == 3) {
                    title = '隐检详情'
                }
                if (type == 4) {
                    title = '成检详情'
                }
                if (type == 5) {
                    title = '发货检验详情'
                }
                var params = {
                    title: title,
                    pcNo: this.dataObj.pcInfo.pcNo,
                    list:type != 1 ? encodeURIComponent(JSON.stringify(options)) : encodeURIComponent(JSON.stringify(rawMaterialsMap[options])),
                    url: ctxPath,
                    status:type==6?1:type
                }
                window.location.href = ctxPath + "/webServer/checkResult" + furlParams(params);
            },
            // 过滤文件服务器图片
            filterFile(fileUrl) {
                var options = {
                    isHttps: false,
                    url: fileUrl
                }
                if (fileUrl && fileUrl.indexOf('https://') > -1) {
                    options.isHttps = true;
                    return options
                } else if (fileUrl && fileUrl != 'undefined') {
                    if (fileUrl[0] == '/') {
                        options.url = ctxPathUrl + fileUrl;
                    } else {
                        options.url = ctxPathUrl + '/' + fileUrl;
                    }
                    return options
                } else {
                    options.url = '';
                    return options
                }
            },
        },
        created: function () {
            this.getCovertCkeckflag()
            // 组模详情跳转过来更新查询
            if (getParams().pcNo && getParams().type == 1) {
            }
        },
    })
</script>



